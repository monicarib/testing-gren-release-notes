{
  "dataSource": "prs",
  "prefix": "",
  "onlyMilestones": false,
  "changelogFilename": "CHANGELOG.md",
  "ignoreIssuesWith": [
    "duplicate",
    "wontfix",
    "invalid",
    "help wanted"
  ],
  "template": {
    "issue": "- [{{text}}]({{url}}) {{name}}"
  },
  "groupBy": {
    "Enhancements:": [
      "enhancement",
      "internal"
    ],
    "Bug Fixes:": [
      "bug"
    ]
  }
}
